         static void end(valu in)
         {
             in.name="마무리 하겠습니다.";
         }



 }
 class valu {

String name="첫번째 클래스";
    

 }
 class vava{

     static void tem(valu in)
     {
         in.name="두번째 클래스";
     }
 }
