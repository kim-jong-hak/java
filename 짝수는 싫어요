class Solution {
    public int[] solution(int n) {
   int n_2=0;
        if(n%2==1)
        {
            n_2=((n/2)+1);
        }else
        {
            n_2=(n/2);
        }
    int num[]=new int[n_2],i_2=0;

    for(int i=0;i<=n;i++)
    {
        if(i%2==1)
        {
            num[i_2]=i;
            i_2++;

        }

    }

        return num;
    }
}


